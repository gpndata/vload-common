{
  "name": "vload/common",
  "description": "VLoad API connector commons",
  "type": "library",
  "require": {
    "php": ">=5.6",
    "guzzlehttp/guzzle": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Vload\\Common\\": "src/",
      "Vload\\CommonTest\\": "tests/"
    }
  },
  "require-dev": {
